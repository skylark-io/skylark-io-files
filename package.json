{
  "name": "skylark-io-files",
  "version": "0.9.0",
  "description": "The skylark file system library",
  "main": "-",
  "scripts": {
    "build": "slib build .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "homepage": "www.skylarkjs.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skylark-data/skylark-data-files.git"
  },
  "keywords": [
    "skylark",
    "skylarkjs",
    "javacript",
    "library",
    "utility",
    "html5"
  ],
  "author": "Hudaokeji Co.,Ltd",
  "license": "MIT",
  "dependencies": {
    "skylark-langx-ns": "https://github.com/skylark-langx/skylark-langx-ns",
    "skylark-langx-strings": "https://github.com/skylark-langx/skylark-langx-strings",
    "skylark-langx-funcs": "https://github.com/skylark-langx/skylark-langx-funcs",
    "skylark-langx-async": "https://github.com/skylark-langx/skylark-langx-async",
    "skylark-langx-paths": "https://github.com/skylark-langx/skylark-langx-paths",
    "skylark-langx-binary": "https://github.com/skylark-langx/skylark-langx-binary",
    "skylark-data-compression": "https://github.com/skylark-data/skylark-data-compression",
    "skylark-data-collections": "https://github.com/skylark-data/skylark-data-collections"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "skylarkjs": {
    "directories": {
      "dependencies": "./node_modules"
    },
    "secondaries": {},
    "build": {
      "prepare": {
        "texttojs": [
          "html",
          "json",
          "css"
        ],
        "jsxtojs": false,
        "es6toamd": false,
        "cjstoamd": false
      },
      "bundles": {
        "all": true
      }
    }
  }
}
